{
    "name": "nmig",
    "version": "6.1.0",
    "description": "The database migration app",
    "author": "Anatoly Khaytovich<anatolyuss@gmail.com>",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/AnatolyUss/nmig.git"
    },
    "engines": {
        "node": ">=18.16.1"
    },
    "scripts": {
        "build": "tsc --incremental -p tsconfig.json",
        "lint": "npx eslint . --ext .ts",
        "format": "npx prettier . --ignore-path ./.prettierignore --write && git status",
        "flb": "npm run format && npm run lint && npm run build",
        "start": "node dist/src/main.js",
        "test": "node dist/test/main.test.js"
    },
    "dependencies": {
        "@types/node": "20.4.4",
        "@types/uuid": "9.0.2",
        "@types/pg": "8.10.2",
        "@types/pg-copy-streams": "1.2.2",
        "json2csv": "5.0.7",
        "mysql2": "3.5.2",
        "pg": "8.11.1",
        "pg-copy-streams": "6.0.6",
        "uuid": "9.0.0"
    },
    "devDependencies": {
        "@types/tape": "5.6.0",
        "@typescript-eslint/eslint-plugin": "5.61.0",
        "@typescript-eslint/parser": "5.61.0",
        "eslint": "8.44.0",
        "prettier": "3.0.0",
        "tape": "5.6.6",
        "typescript": "5.1.6"
    },
    "keywords": [
        "db migration",
        "database migration",
        "databases migration",
        "mysql",
        "mariadb",
        "copy",
        "postgresql",
        "postgres",
        "postgre",
        "pg"
    ]
}
