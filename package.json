  },
  "devDependencies": {
    "@types/tape": "5.6.4",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "@typescript-eslint/parser": "8.0.1",
    "eslint": "8.57.0",
    "prettier": "3.3.3",
    "tape": "5.8.1",
